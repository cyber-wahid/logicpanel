{
    "name": "logicpanel/control-panel",
    "description": "Secure shared-hosting control panel for Node.js and Python applications",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.2",
        "dragonmantank/cron-expression": "^3.6",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/database": "^10.0",
        "league/flysystem": "^3.23",
        "monolog/monolog": "^3.5",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "symfony/process": "^6.4",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "LogicPanel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "stan": "phpstan analyse src"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}